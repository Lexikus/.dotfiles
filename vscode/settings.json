{
    "files.exclude": {
        "**/.git": true,
        "**/.collabignore": true,
        "**/.DS_Store": true,
        "**/*.meta": true,
        "**/*.*.meta": true,
        "**/*.unity": true,
        "**/*.unityproj": true,
        "**/*.mat": true,
        "**/*.fbx": true,
        "**/*.FBX": true,
        "**/*.tga": true,
        "**/*.cubemap": true,
        "**/*.prefab": true,
        "**/Library": true,
        "**/ProjectSettings": true,
        "**/Temp": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.vs": true,
        "**/.vscode": true,
        "**/.idea": true,
        "**/__snapshots__": true,
        "**/node_modules": true
    },
    "terminal.integrated.shellArgs.linux": ["-l"],
    "editor.fontFamily": "Fira Code, Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.codeLens": false,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.formatOnType": true,
    "window.zoomLevel": 0,
    "files.trimTrailingWhitespace": true,
    "workbench.iconTheme": "vs-one-dark-icons",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.renderWhitespace": "boundary",
    "editor.dragAndDrop": false,
    "editor.minimap.enabled": false,
    "files.trimFinalNewlines": true,
    "workbench.startupEditor": "none",
    "workbench.statusBar.feedback.visible": false,
    "workbench.colorTheme": "VS One Dark Theme",
    "window.titleBarStyle": "custom",
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "breadcrumbs.enabled": true,
    "explorer.confirmDelete": false,
    "editor.fontSize": 13.8,
    "javascript.preferences.quoteStyle": "single",
    "typescript.preferences.quoteStyle": "single",
    "docthis.inferTypesFromNames": true,
    "docthis.includeTypes": true,
    "docthis.includeMemberOfOnInterfaceMembers": true,
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#b52e31"
        },
        {
            "name": "Auth0 Orange",
            "value": "#eb5424"
        },
        {
            "name": "Azure Blue",
            "value": "#007fff"
        },
        {
            "name": "C# Purple",
            "value": "#68217A"
        },
        {
            "name": "Gatsby Purple",
            "value": "#639"
        },
        {
            "name": "Go Cyan",
            "value": "#5dc9e2"
        },
        {
            "name": "Java Blue-Gray",
            "value": "#557c9b"
        },
        {
            "name": "JavaScript Yellow",
            "value": "#f9e64f"
        },
        {
            "name": "Mandalorian Blue",
            "value": "#1857a4"
        },
        {
            "name": "Node Green",
            "value": "#215732"
        },
        {
            "name": "React Blue",
            "value": "#00b3e6"
        },
        {
            "name": "Something Different",
            "value": "#832561"
        },
        {
            "name": "Vue Green",
            "value": "#42b883"
        }
    ],
    "todohighlight.keywords": [
        {
            "text": "NOTE:",
            "color": "white",
            "backgroundColor": "#F736BF"
        },
        {
            "text": "TODO:",
            "color": "white",
            "backgroundColor": "#F77F6B"
        }
    ],
    "editor.detectIndentation": false,
    "explorer.confirmDragAndDrop": false,
    "color-highlight.markerType": "dot-before",
    "color-highlight.markRuler": false,
    "color-highlight.languages": [
        "*", "!css", "!scss", "!sass"
    ],
    "liveServer.settings.donotShowInfoMsg": true,
    "typescript.preferences.importModuleSpecifier": "relative"
}